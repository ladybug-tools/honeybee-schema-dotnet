{
  "name": "honeybee-schema",
  "license": "MIT",
  "version": "1.5804.6",
  "description": "Honeybee Schema's typescript version",
  "author": "Ladybug Tools",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "check": "tsc --noEmit",
    "build:tsc": "npm run clean && tsc --build",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:esbuild": "esbuild src/index.ts --bundle --outdir=dist --sourcemap --format=esm",
    "build": "npm run clean && npm run check && npm run bundle:dts && npm run build:esbuild",
    "bundle:dts": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "custom-pack": "npm run build && npm pack",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.24.0",
    "gts": "^5.3.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2"
  }
}
