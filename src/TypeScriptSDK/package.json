{
  "name": "honeybee-schema",
  "license": "MIT",
  "version": "1.5804.0",
  "description": "Honeybee Schema's typescript version",
  "author": "Ladybug Tools",
  "type": "module",
  "files": ["dist"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "bundle": "tsc --build && esbuild index.ts --bundle --outfile=dist/index.js --loader:.ts=ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "tsc",
    "pretest": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "gts": "^5.3.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "class-transformer": "^0.5.1"
  }
}
