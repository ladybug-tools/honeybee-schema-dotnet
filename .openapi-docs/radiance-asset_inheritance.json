      "Wea": {
        "allOf": [
          {
            "$ref": "#/components/schemas/_OpenAPIGenBaseModel"
          },
          {
            "type": "object",
            "properties": {
              "location": {
                "title": "Location",
                "description": "Location object to note latitude, longitude and time zone.",
